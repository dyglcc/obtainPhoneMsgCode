<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="boolean" name="autoPlaying"/>
    <attr format="integer" name="interval"/>
    <attr format="enum" name="orientation">
        <enum name="horizontal" value="0"/>
        <enum name="vertical" value="1"/>
    </attr>
    <attr format="boolean" name="showIndicator"/>
    <string name="app_name">library</string>
    <declare-styleable name="BannerLayout">
        <attr name="interval"/>
        <attr name="showIndicator"/>
        <attr name="orientation"/>
        <attr name="autoPlaying"/>
        <attr format="integer" name="itemSpace"/>
        <attr format="float" name="centerScale"/>
        <attr format="float" name="moveSpeed"/>
    </declare-styleable>
    <declare-styleable name="RecyclerViewBannerBase">
        <attr name="interval"/>
        <attr name="showIndicator"/>
        <attr name="orientation"/>
        <attr name="autoPlaying"/>
        <attr format="color|reference" name="indicatorSelectedSrc"/>
        <attr format="color|reference" name="indicatorUnselectedSrc"/>
        <attr format="dimension|reference" name="indicatorSpace"/>
        <attr format="dimension|reference" name="indicatorMarginLeft"/>
        <attr format="dimension|reference" name="indicatorMarginRight"/>
        <attr format="dimension|reference" name="indicatorMarginBottom"/>
        <attr format="enum" name="indicatorGravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>
</resources>